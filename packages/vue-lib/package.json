{
  "name": "@ignt/vue-library",
  "version": "0.4.3",
  "description": "A library of Vue 3 components for building frontends for cosmos-sdk chains",
  "repository": "https://github.com/spellshape/web.git",
  "author": "Spellshape, Inc <hello@spellshape.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/vue-library.umd.js",
  "module": "./dist/vue-library.mjs",
  "types": "./dist/index.d.ts",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/vue-library.mjs",
      "require": "./dist/vue-library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "run-s type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "bignumber.js": "^9.1.0",
    "crypto-js": "^4.1.1",
    "gradient-avatar": "^1.0.2",
    "qrcode": "^1.5.1"
  },
  "peerDependencies": {
    "vue": "^3.2.38",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.11.8",
    "@types/qrcode": "^1.5.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.2",
    "json": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4",
    "vite": "^3.2.1",
    "vite-plugin-dts": "^1.6.6",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
  }
}